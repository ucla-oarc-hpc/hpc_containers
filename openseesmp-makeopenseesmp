RMFLAGS         = -f
SHELL           = /bin/sh
.SUFFIXES:
.SUFFIXES:      .C .c .f .f90 .cpp .o .cpp
.DEFAULT:
	@$(ECHO) "Unknown target $@, try:  make help"
.cpp.o:
	@$(ECHO) Making $@ from $<
	$(CC++) $(C++FLAGS) $(INCLUDES) -c $< -o $@

.C.o:
	@$(ECHO) Making $@ from $<
	$(CC++) $(C++FLAGS) $(INCLUDES) -c $< -o $@
.c.o:
	@$(ECHO) Making $@ from $<
	$(CC) $(CFLAGS) -c $< -o $@
.f.o:
	@$(ECHO) Making \$@ from $<
	$(FC) $(FFLAGS) -c $< -o $@
